{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "kim-gui",
  "version": "2.0.6",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cy:open": "yarn cypress open",
    "cy:run": "yarn cypress run --browser chrome",
    "cy:run:cp": "yarn cypress run --component"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4",
    "@cypress/code-coverage": "^3.14.4",
    "@cypress/react": "^9.0.1",
    "@cypress/vite-dev-server": "^6.0.3",
    "@eslint/js": "^9.25.0",
    "@storybook/addon-a11y": "9.0.5",
    "@storybook/addon-docs": "9.0.5",
    "@storybook/addon-onboarding": "9.0.5",
    "@storybook/addon-vitest": "9.0.5",
    "@storybook/react-vite": "9.0.5",
    "@tailwindcss/postcss": "^4.1.8",
    "@tailwindcss/vite": "^4.1.8",
    "@testing-library/cypress": "^10.0.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.30",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.2.2",
    "@vitest/coverage-v8": "^3.2.2",
    "cypress": "^14.4.1",
    "cypress-mochawesome-reporter": "^3.8.2",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "9.0.5",
    "globals": "^16.0.0",
    "playwright": "^1.52.0",
    "storybook": "9.0.5",
    "tailwindcss": "^4.1.8",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-istanbul": "^7.0.0",
    "vitest": "^3.2.2"
  },
  "main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "msw"
  ],
  "prepublishOnly": "yarn build"
}
